{
	"name": "lunascript-tool",
	"description": "Luna Script IDE Tools",
	"version": "0.0.1",
	"publisher": "lunaluxprojects",
	"repository": "https://https://github.com/LunaLuxProjects/LibLunaScript/plugin",
	"engines": {
		"vscode": "^1.32.0"
	},
	"activationEvents": [
		"onCommand:extension.helloWorld"
	],
	"main": "./extension.js",
	"categories": [
        "Languages"
    ],
    "contributes": {
        "languages": [{
            "id": "lunascript",
            "aliases": ["lls", "lunascript"],
            "extensions": [".lls"],
            "configuration": "./language-configuration.json"
        }],
        "grammars": [{
            "language": "lunascript",
            "scopeName": "source.lls",
            "path": "./syntaxes/lunascript.tmLanguage.json"
        }]
    },
	"scripts": {},
	"devDependencies": {
		"@types/vscode": "^1.32.0"
	}
}
